# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
charset = utf-8
# Unix-style newlines
end_of_line = lf
# a newline ending every file
insert_final_newline = true
# no whitespace at end of line
trim_trailing_whitespace = true
# indent with tabs
indent_style = tab
